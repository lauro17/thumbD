{
    "manifest_version": 3,
    "name": "thunbD",
    "description": "Permite baixar a thunbemail de videos do youtube",
    "version": "0.0.1",
    "author":"LUIZ LAURO SILVA GONAÃ‡ALVES",
    "action": {
        "default_icon": "icons/icone16.png",
